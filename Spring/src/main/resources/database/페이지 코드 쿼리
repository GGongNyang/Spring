CREATE TABLE SPR_PAGECODE(
    PCODE NUMBER PRIMARY KEY,
    PNAME VARCHAR2(20) NOT NULL,
    PTYPE VARCHAR2(20) NOT NULL
);

COMMENT ON TABLE SPR_PAGECODE IS '페이지 코드';
COMMENT ON COLUMN SPR_PAGECODE.PCODE IS '페이지 코드';
COMMENT ON COLUMN SPR_PAGECODE.PNAME IS '페이지 이름';
COMMENT ON COLUMN SPR_PAGECODE.PTYPE IS '페이지 타입';